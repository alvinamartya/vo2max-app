        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">
        <activity
            android:name=".Activity.BeepPelatihActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.StopWatchActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.AtlitProfileActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.PelatihProfileActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.BeepDataActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.BeepAtlitActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name=".Activity.BeepStartActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.BeepMetodeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.BalkeMetodeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
